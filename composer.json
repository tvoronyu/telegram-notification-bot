{
    "name": "voronyuk/laravel-telegram-notifier",
    "description": "Flexible Telegram notification system for Laravel with support for different message types, queues, and specialized classes",
    "keywords": [
        "laravel",
        "telegram",
        "notifications",
        "telegram-bot",
        "laravel-package"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taras Voronyuk",
            "email": "taras@voronyuk.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "10.0",
        "illuminate/notifications": "10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Voronyuk\\TelegramNotifier\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Voronyuk\\TelegramNotifier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voronyuk\\TelegramNotifier\\TelegramNotifierServiceProvider"
            ],
            "aliases": {
                "Telegram": "Voronyuk\\TelegramNotifier\\Facades\\Telegram"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
