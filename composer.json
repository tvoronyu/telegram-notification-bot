{
    "name": "voronyuk/laravel-telegram-notifier",
    "description": "Flexible Telegram notification system for Laravel and Lumen with support for different message types, queues, and specialized classes",
    "keywords": [
        "laravel",
        "lumen",
        "telegram",
        "notifications",
        "telegram-bot",
        "laravel-package",
        "lumen-package"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taras Voronyuk",
            "email": "taras@voronyuk.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Voronyuk\\TelegramNotifier\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Voronyuk\\TelegramNotifier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voronyuk\\TelegramNotifier\\TelegramNotifierServiceProvider"
            ],
            "aliases": {
                "Telegram": "Voronyuk\\TelegramNotifier\\Facades\\Telegram"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
